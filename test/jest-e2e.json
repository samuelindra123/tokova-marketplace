{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "testTimeout": 60000,
  "transform": {
    "^.+\\.(t|j)s$": [
      "ts-jest",
      {
        "useESM": true
      }
    ]
  },
  "extensionsToTreatAsEsm": [
    ".ts"
  ],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1",
    "^uuid$": "uuid"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(uuid|slugify)/)"
  ]
}